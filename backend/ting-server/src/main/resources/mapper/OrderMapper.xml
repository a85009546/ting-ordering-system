<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.github.mattwei.mapper.OrderMapper">


    <insert id="insert" useGeneratedKeys="true" keyProperty="id">
        insert into orders(number,status,user_id,address_book_id,order_time,pay_status,amount,
                           remark,phone,address,user_name,consignee,rejection_reason,
                           estimated_delivery_time,delivery_status,delivery_time)
        values(#{number},#{status},#{userId},#{addressBookId},#{orderTime},#{payStatus},
               #{amount},#{remark},#{phone},#{address},#{userName},#{consignee},
               #{rejectionReason},#{estimatedDeliveryTime},#{deliveryStatus},#{deliveryTime})
    </insert>
</mapper>